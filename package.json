  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc --properties es2015 browser module main --first-only",
    "start": "ng serve",
    "build": "ng build --prod --aot --build-optimizer --common-chunk --vendor-chunk --optimization --progress",
    "build-dev": "ng build --configuration dev --aot --build-optimizer --common-chunk --vendor-chunk --optimization --progress",
    "build-idsmeduat": "ng build --configuration idsmeduat --aot --build-optimizer --common-chunk --vendor-chunk --optimization --progress",
    "build-idsmedprod": "ng build --configuration idsmedprod --aot --build-optimizer --common-chunk --vendor-chunk --optimization --progress",
    "build-idsmeddev": "ng build --configuration idsmeddev --aot --build-optimizer --common-chunk --vendor-chunk --optimization --progress",
    "build-docs": "npx compodoc -p tsconfig.doc.json",
    "test": "ng test",
    "test-dms": "npx ng test --include=**/dms/**/*.spec.ts --include=**/dms/*.spec.ts",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
